--Properties:
local function makeStroke(int)
	local stroke = Instance.new('UIStroke',int)
	stroke.Color = Color3.fromRGB(200,200,200)
	stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	stroke.LineJoinMode = Enum.LineJoinMode.Round
	stroke.Thickness = 5
	stroke.Transparency = 0
	stroke.Enabled = true
end
--Properties:
makeStroke(Button)
makeStroke(Title)
makeStroke(main)
